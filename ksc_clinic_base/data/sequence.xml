<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_hms_patient" model="ir.sequence">
            <field name="name">KSC Patient</field>
            <field name="code">patient</field>
            <field name="prefix">PT</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_acs_physician" model="ir.sequence">
            <field name="name">KSC Physician</field>
            <field name="code">physician</field>
            <field name="prefix">DR</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_hms_patient_evaluation" model="ir.sequence">
            <field name="name">ksc Patient Evaluation</field>
            <field name="code">ksc.patient.evaluation</field>
            <field name="prefix">EVA</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
